<!DOCTYPE html>
<html lang="en">

<head>
        <meta charset="UTF-8" />
        <title>Your map</title> 
  <script src="http://code.jquery.com/jquery-1.4.2.min.js"></script>

	<script src="http://maps.google.com/maps/api/js?sensor=false&amp;key=x" type="text/javascript"></script>

</head>

<body style="width: 990px; margin: auto;">
	<div id="container" style="min-height: 500px; padding: 5px;">
		
<script type="text/javascript">

var get_tile_url = function(point, zoom) {
	return "/data/tiles/tile_" + zoom.toString() + "_" + point.x + "_" + point.y + ".png";
}

var map;
var geodan = new google.maps.LatLng(52.34221,4.913162);
var latlng = new google.maps.LatLng(42.333118683379396, -71.0841724921875);
var osmMapType = new google.maps.ImageMapType({
	getTileUrl: get_tile_url,
	tileSize: new google.maps.Size(256, 256),
	isPng: true,
	maxZoom: 4,
	minZoom: 0
});
function initialize_gmap() {
	var mapOptions = {
		zoom: 2,
		center: latlng,
		mapTypeId: 'OSM',
		mapTypeControlOptions: {
			mapTypeIds: ['OSM', google.maps.MapTypeId.ROADMAP],
			style: google.maps.MapTypeControlStyle.DROPDOWN_MENU
		},
		scrollwheel: false,
		navigationControlOptions: {
			style: google.maps.NavigationControlStyle.ZOOM_PAN /* ZOOM_PAN, SMALL */
		},
		
	};
	map = new google.maps.Map(document.getElementById("map_canvas"),mapOptions);
	map.mapTypes.set('OSM',osmMapType);
	map.setMapTypeId('OSM');


}


jQuery(document).ready(function() {
	initialize_gmap();
});
</script>

<div style="width: 600px; height: 400px; margin-bottom: 10px; border: 1px solid #aaa;" id="map_canvas"></div>





	</div>
</body>

</html>
